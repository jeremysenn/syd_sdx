$('#workorders_list').append('<%= j render "workorders/workorder" %>');
$('#more_workorders_spinner').hide();

<% if @workorders.next_page %>
  $('a.load-more-workorders').show();
  $('a.load-more-workorders').attr('href', '<%= customer_path id: @customer['Id'], page: @workorders.next_page %>');
<% else %>
  $(window).off('scroll');
  $('a.load-more-workorders').attr('href', '#');
  $('a.reload-workorders').show();
<% end %>