.navbar.navbar-default.navbar-fixed-top{ role: 'navigation'}
  .container-fluid
    .navbar-header
      %button.navbar-toggle{ type: 'button', data: { toggle: 'collapse', target: '.navbar-collapse' } }
        %span.sr-only= t('.toggle_navigation')
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar
      %a{class: 'navbar-brand', :href => root_path}
        = image_tag('syd_logo_20x37.png')
    .collapse.navbar-collapse
      %ul.nav.navbar-nav
        %li
          %a{href: images_path}
            PICTURES
          
      %ul.nav.navbar-nav.navbar-right
        %li= link_to "User: <strong>#{current_user.username}</strong>".html_safe, current_user
        %li= link_to 'Log out', logout_path, method: :delete, data: { confirm: 'Are you sure you want to log out?' }